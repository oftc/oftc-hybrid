# $Id$
# makefile for include/

INSTALL		= @INSTALL@
INSTALL_DATA	= @INSTALL_DATA@
RM		= @RM@

prefix		= @prefix@

all:

install:
	mkdir -p $(prefix)/include/ircd-hybrid-7
	@echo Installing header files...

	@for header in *.h; do \
		${INSTALL_DATA} $$header $(prefix)/include/ircd-hybrid-7; \
	done

distclean:
	${RM} -f Makefile

.PHONY: install distclean
