# $Id: Makefile.in 497 2006-05-27 23:32:21Z stu $

CC = @CC@
LIBS = @LIBS@
CFLAGS = @CFLAGS@
INCLUDES = -I@INCLUDES@

all: respond

respond: respond.c
	$(CC) $(CFLAGS) -o respond respond.c ${INCLUDES} ${LDFLAGS} ${LIBS}

clean:
	rm -f respond *.core *~ *.exe

distclean: clean
	rm -f Makefile config.status config.log
	rm -rf autom4te.cache
